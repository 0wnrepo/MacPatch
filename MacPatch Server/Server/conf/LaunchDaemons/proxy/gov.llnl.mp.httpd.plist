<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN"
 "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
	<dict>
	<key>Label</key>
	<string>gov.llnl.mp.httpd</string>
	<key>ServiceDescription</key>
	<string>MacPatch Apache2 httpd web server</string>
	<key>ProgramArguments</key>
	<array>
	   <string>/Library/MacPatch/Server/Apache2/bin/httpd</string>
	   <string>-D</string>
           <string>FOREGROUND</string>
	</array>
	<key>RunAtLoad</key>
	<true/>
	<key>StandardErrorPath</key>
        <string>/Library/MacPatch/Server/Apache2/logs/stderr.log</string>
        <key>StandardOutPath</key>
        <string>/Library/MacPatch/Server/Apache2/logs/stdout.log</string>
	</dict>
</plist>
