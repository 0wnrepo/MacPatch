<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>gov.llnl.mp.ProxySync</string>
	<key>ServiceDescription</key>
	<string>MacPatch Proxy Content Sync</string>
	<key>ProgramArguments</key>
	<array>
		<string>/Library/MacPatch/Server/conf/scripts/proxy/MPProxySync.py</string>
		<string>--plist</string>
		<string>/Library/MacPatch/Server/conf/etc/proxy/gov.llnl.MPProxySync.plist</string>
	</array>
	<key>StartInterval</key>
	<integer>1800</integer>
	<key>RunAtLoad</key>
	<true/>
	<key>StandardOutPath</key>
	<string>/Library/MacPatch/Server/Logs/ProxySync_stdout.log</string>
	<key>StandardErrorPath</key>
	<string>/Library/MacPatch/Server/Logs/ProxySync_stderr.log</string>
</dict>
</plist>
