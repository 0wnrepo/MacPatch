<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Disabled</key>
	<false/>
	<key>Label</key>
	<string>gov.llnl.mp.AVDefsSync</string>
	<key>ProgramArguments</key>
	<array>
		<string>/Library/MacPatch/Server/conf/scripts/MPAVDefsSync.py</string>
