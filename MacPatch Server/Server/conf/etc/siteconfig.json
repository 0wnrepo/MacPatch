{
    "settings": 
    {
        "services":
        {
            "console":false,
            "mpwsl":false
        },
        "paths": 
        {
            "base":"/Library/MacPatch",
            "content":"/Library/MacPatch/Content/Web"
        },
        "users": 
        {
            "admin": {
                "enabled": "YES",
                "name":"mpadmin",
                "pass":"*mpadmin*"
            }    
        },
        "ldap": 
        {
            "server":"SERVERNAME",
            "searchbase":"SEARCH-BASE",
            "attributes":"givenname,initials,sn,mail,memberOf,dn,samAccountName,userPrincipalName",
            "port":"SERVER-PORT",
            "secure":"AD-DOMAIN-SSL",
            "loginAttr":"LOGIN-ATTRIBUTE",
            "loginUsrPrefix":"LOGIN-PREFIX",
            "loginUsrSufix":"LOGIN-SUFFIX"
        },
        "database": 
        {
            "prod": {
                "enabled": "YES",
                "dsName":"mpds",
                "dbHost":"127.0.0.1",
                "dbPort":"3306",
                "dbName":"MacPatchDB",
                "hoststringPre":"jdbc:mysql:",
                "hoststringURI":"?cacheResultSetMetadata=false&amp;autoReconnect=true&amp;zeroDateTimeBehavior=convertToNull",
                "drivername":"com.mysql.jdbc.Driver",
                "username":"mpdbadm",
                "password":"",
                "maxconnections":500,
                "logintimeout":30,
                "connectiontimeout":30,
                "connectionretries":2
            },
            "ro": {
                "enabled": "YES",
                "dsName":"mpdsro",
                "dbHost":"127.0.0.1",
                "dbPort":"3306",
                "dbName":"MacPatchDB",
                "hoststringPre":"jdbc:mysql:",
                "hoststringURI":"?cacheResultSetMetadata=false&amp;autoReconnect=true&amp;zeroDateTimeBehavior=convertToNull",
                "drivername":"com.mysql.jdbc.Driver",
                "username":"mpdbro",
                "password":"",
                "maxconnections":100,
                "logintimeout":30,
                "connectiontimeout":30,
                "connectionretries":2
            }
        },
        "ldap_filters": 
        [
            {   "config_name":"DEMO_CONFIG",
                "config_ldap": { 
                    "server": "SERVERNAME.COM",
                    "port": 636,
                    "secure": "1", 
                    "searchbase": "SEARCH-BASE",  
                    "attributes": "cn,name,dn,samAccountName,memberOf",
                    "userDN": "USER DN PATH",
                    "userPas": "USER PASSWORD"
                }
            }
        ],
        "mailserver": 
        {
            "enabled": "YES",
            "server":"127.0.0.1",
            "username":"exampleusername",
            "password":"examplepassword"
        },
        "dbSchema": 
        {
            "checkSchema": false
        }
    }
}
