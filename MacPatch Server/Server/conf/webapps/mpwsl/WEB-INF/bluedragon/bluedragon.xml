<?xml version="1.0" encoding="UTF-8"?>
<server>
  <system>
    <buffersize>0</buffersize>
    <strictarraypassbyreference>false</strictarraypassbyreference>
    <password>admin</password>
    <licensekey></licensekey>
    <assert>false</assert>
    <missingtemplatehandler></missingtemplatehandler>
    <tempdirectory>/WEB-INF/bluedragon/work/temp</tempdirectory>
    <deniedips></deniedips>
    <defaultcharset>UTF-8</defaultcharset>
    <component-cfc>/WEB-INF/bluedragon/component.cfc</component-cfc>
    <scriptsrc>/bluedragon/scripts</scriptsrc>
    <formurlcombined>false</formurlcombined>
    <servercfc>/Server/Server.cfc</servercfc>
    <legacyformvalidation>false</legacyformvalidation>
    <scriptprotect>false</scriptprotect>
    <nativelibdir>/WEB-INF/bin</nativelibdir>
    <errorhandler></errorhandler>
    <functionscopedvariables>false</functionscopedvariables>
    <allowedips></allowedips>
    <whitespacecomp>true</whitespacecomp>
  </system>
  <file>
    <maxfiles>1000</maxfiles>
    <trustcache>false</trustcache>
  </file>
  <javacustomtags>
    <classes>/WEB-INF/classes</classes>
  </javacustomtags>
  <nativecustomtags>
  </nativecustomtags>
  <cfchart>
    <cachesize>1000</cachesize>
    <storage>file</storage>
  </cfchart>
  <fonts>
    <dirs>C:\Windows\Fonts</dirs>
  </fonts>
  <cfquery>
    <cache>true</cache>
    <cachecount>1000</cachecount>
  </cfquery>
  <openbdadminapi>
    <version>2.0</version>
    <lastupdated>31/Jul/2012 13:59:47</lastupdated>
  </openbdadminapi>
  <cfapplication>
    <j2eesession>true</j2eesession>
    <clientglobalupdatesdisabled>true</clientglobalupdatesdisabled>
    <applicationtimeout>#CreateTimeSpan(2,0,0,0)#</applicationtimeout>
    <clientpurgeenabled>true</clientpurgeenabled>
    <sessiontimeout>#CreateTimeSpan(0,4,0,0)#</sessiontimeout>
    <cf5clientdata>false</cf5clientdata>
    <clientexpiry>90</clientexpiry>
    <enabled>true</enabled>
    <clientstorage>cookie</clientstorage>
  </cfapplication>
  <cfcachecontent>
    <datasource></datasource>
    <total>1000</total>
  </cfcachecontent>
  <cfmail>
    <interval>240</interval>
    <smtpserver>127.0.0.1</smtpserver>
    <charset>UTF-8</charset>
    <smtpport>25</smtpport>
  </cfmail>
  <cfmlcustomtags>
    <mapping name="cf">
      <directory>/WEB-INF/customtags</directory>
      <name>cf</name>
    </mapping>
  </cfmlcustomtags>
</server>
