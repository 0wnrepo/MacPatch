<?xml version="1.0"?>
<settings>
	<paths>
		<base>/Library/MacPatch</base>
		<content>/Library/MacPatch/Content/Web</content>
	</paths>
	<admin>
		<mailto>
			<enable>0</enable>
			<email>admin@example.com</email>
		</mailto>
		<ip>
			<allow>192.1.168.</allow>
		</ip>
	</admin>
	<mailserver>
		<server>127.0.0.1</server>
		<username>exampleusername</username>
		<password>examplepassword</password>
	</mailserver>
	<proxyServer>
		<primaryServer>[MP_SERVER]</primaryServer>
		<primaryServerPort>[MP_SERVER_PORT]</primaryServerPort>
		<serverKey>autoGen</serverKey>
		<seedKey>[SEED_KEY]</seedKey>
	</proxyServer>
</settings>
